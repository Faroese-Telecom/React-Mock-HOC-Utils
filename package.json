{
  "name": "react-mock-hoc-utils",
  "version": "1.0.0",
  "description": "\"A Higher-Order-Component mocking tool that allows for mocking any component that is wrapped by one or more HOCs - additionally, supporting prop injection on a per-hoc-basis. Is built for React and Jest.",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && node node_modules/babel-cli/bin/babel.js src --out-dir lib",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Faroese-Telecom/React-Mock-HOC-Utils.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "react",
    "jest",
    "testing",
    "higher order components",
    "shallow",
    "diving",
    "enzyme"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Faroese-Telecom/React-Mock-HOC-Utils/issues"
  },
  "homepage": "https://github.com/Faroese-Telecom/React-Mock-HOC-Utils#readme",
  "dependencies": {
    "callsite": "^1.0.0",
    "enzyme": "^3.8.0",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "enzyme-adapter-react-15": "^1.3.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "enzyme-matchers": "^7.0.1",
    "enzyme-to-json": "^3.3.5",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "jest-enzyme": "^7.0.1",
    "jest-junit": "^6.2.1",
    "jsdom-global": "^3.0.2",
    "react": "^16.7.0",
    "react-dom": "^16.8.4",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.3",
    "test-component-builder": "^0.2.3"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/setupTest.js"
    ]
  }
}
